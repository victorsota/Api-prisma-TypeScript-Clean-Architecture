// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model equipamento {
  id    Int     @id @default(autoincrement())
  name String  @unique
  descrition  String?
}

model tipo {
  id        Int     @id @default(autoincrement())
  nome     String
  equipamentos Equipamento[]
}